{
  "name": "hitoka",
  "version": "0.5.1",
  "description": "Hitoka is an eslint, prettier and typescript initializer.",
  "bin": {
    "hitoka": "build/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "files": [
    "/template",
    "/build/src",
    "!/build/src/**/*.map",
    ".eslintrc.js",
    ".prettierrc.js",
    "tsconfig.base.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pacexy/hitoka.git"
  },
  "keywords": [],
  "author": "pacexy",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pacexy/hitoka/issues"
  },
  "homepage": "https://github.com/pacexy/hitoka#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "type-fest": "^0.19.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
