{
  "name": "hitoka",
  "version": "0.4.0",
  "description": "Hitoka is an eslint, prettier and typescript initializer.",
  "bin": {
    "hitoka": "build/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "files": [
    "/template",
    "/build/src",
    "!/build/src/**/*.map",
    ".eslintrc.js",
    ".prettierrc.js",
    "tsconfig.base.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pacexy/hitoka.git"
  },
  "keywords": [],
  "author": "pacexy",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pacexy/hitoka/issues"
  },
  "homepage": "https://github.com/pacexy/hitoka#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "type-fest": "^0.18.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
