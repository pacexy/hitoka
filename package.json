{
  "name": "hitoka",
  "version": "0.6.2",
  "description": "Hitoka is an eslint, prettier and typescript initializer.",
  "bin": {
    "hitoka": "build/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "files": [
    "/template",
    "/build/src",
    "!/build/src/**/*.map",
    ".eslintrc.js",
    ".prettierrc.js",
    ".huskyrc.js",
    ".lintstagedrc.js",
    "tsconfig.base.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pacexy/hitoka.git"
  },
  "keywords": [
    "eslint",
    "prettier",
    "typescript"
  ],
  "author": "pacexy",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pacexy/hitoka/issues"
  },
  "homepage": "https://github.com/pacexy/hitoka#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lodash": "^4.17.21",
    "type-fest": "^0.21.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  }
}
