{
  "name": "hitoka",
  "version": "0.5.2",
  "description": "Hitoka is an eslint, prettier and typescript initializer.",
  "bin": {
    "hitoka": "build/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "files": [
    "/template",
    "/build/src",
    "!/build/src/**/*.map",
    ".eslintrc.js",
    ".prettierrc.js",
    "tsconfig.base.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pacexy/hitoka.git"
  },
  "keywords": [
    "eslint",
    "prettier",
    "typescript"
  ],
  "author": "pacexy",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pacexy/hitoka/issues"
  },
  "homepage": "https://github.com/pacexy/hitoka#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "lodash": "^4.17.20",
    "type-fest": "^0.20.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.12",
    "eslint": "^7.15.0",
    "prettier": "^2.2.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
