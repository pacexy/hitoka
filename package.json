{
  "name": "hitoka",
  "version": "0.7.0",
  "description": "Hitoka is an eslint, prettier and typescript initializer.",
  "bin": {
    "hitoka": "out/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "files": [
    "/out/src",
    "!/out/src/**/*.map",
    ".eslintrc.js",
    ".prettierrc.js",
    ".huskyrc.js",
    ".lintstagedrc.js",
    "tsconfig.json",
    "jest.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pacexy/hitoka.git"
  },
  "keywords": [
    "eslint",
    "prettier",
    "typescript"
  ],
  "author": "pacexy",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pacexy/hitoka/issues"
  },
  "homepage": "https://github.com/pacexy/hitoka#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "type-fest": "^1.2.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.5",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
